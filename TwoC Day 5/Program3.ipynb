   "source": [
    "x=int(input(\"Enter the number of Houses :\"))\n",
    "l=[]\n",
    "for i in range(x):\n",
    "    l.append(int(input(\"Enter the house {} value :\".format(i+1))))\n",
    "a=max(l)\n",
    "b=l.index(a)\n",
    "a=0\n",
    "for i in range(b,x,2):\n",
    "    if l[i]==max(l):\n",
    "        a+=l[i]\n",
    "print(a)"
